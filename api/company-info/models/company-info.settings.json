{
  "connection": "default",
  "collectionName": "company_infos",
  "info": {
    "name": "Company Info"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "office_address": {
      "type": "string"
    },
    "phone": {
      "type": "string"
    },
    "email": {
      "type": "string"
    },
    "factory_address": {
      "type": "string"
    },
    "logo": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": true
    },
    "link_google_map": {
      "type": "string",
      "required": true
    }
  }
}
